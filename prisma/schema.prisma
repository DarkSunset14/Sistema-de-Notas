generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model rol {
  id      Int       @id
  rol     String?
  usuario usuario[]
}

model usuario {
  id Int @id  @default(autoincrement())
  cedula   String  @unique
  apellido String?
  id_Rol   Int?
   created_at DateTime @db.Timestamptz(6)
  nombre   String?
  password String?
  rol      rol?    @relation(fields: [id_Rol], references: [id])
}
